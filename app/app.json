{
  "expo": {
    "name": "vCarbon",
    "slug": "vCarbon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/images/cropped.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "https://i.ibb.co/RD7rQq4/LOGO.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/images/cropped.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.duc.phan2408.vCarbon",
      "config": {"googleMaps": {"apiKey": "AIzaSyBmyyh1Kry00sgWeOJFJ75T3ubwghbXyDg"}}
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b3d00dc3-7182-4433-bdd9-e7b2e8408934"
      }
    }
  }
}
